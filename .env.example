# ============================================================================
# AIDE Environment Variables
# ============================================================================
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# ============================================================================
# AI Provider (Gemini)
# ============================================================================
GEMINI_API_KEY=your_gemini_api_key_here

# ============================================================================
# Database (Development)
# ============================================================================
# When using Docker Compose, this is auto-configured
# For local development without Docker:
# DATABASE_URL=postgresql://aide:aide@localhost:5432/aide

# ============================================================================
# Database (Test)
# ============================================================================
# TEST_DATABASE_URL=postgresql://aide:aide@localhost:5433/aide_test

# ============================================================================
# Application Settings
# ============================================================================
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=debug
LOG_TO_FILE=logs/app.log
LOG_JSON=true
JUDGE0_URL=
SUPABASE_URL=
SUPABASE_KEY=
DATABASE_URL=

# ============================================================================
# Service URLs (Future)
# ============================================================================
# JUDGE0_URL=http://localhost:2358
# REDIS_URL=redis://localhost:6379

# ============================================================================
# Security (Production Only)
# ============================================================================
# SECRET_KEY=your_secret_key_here
# ALLOWED_HOSTS=localhost,127.0.0.1
# CORS_ORIGINS=http://localhost:3000
